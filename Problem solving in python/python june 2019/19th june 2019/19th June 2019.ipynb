    "# seating arrangement \n",
    "\n",
    "t=int(input())\n",
    "for i in range(1,t+1):\n",
    "    seatnum=int(input())\n",
    "    n=(seatnum-1)//12\n",
    "    if seatnum%3==2:\n",
    "        s=\"MS\"\n",
    "    elif seatnum%6<2:\n",
    "        s=\"WS\"\n",
    "    else:\n",
    "        s=\"AS\"\n",
    "    print(24*n+13-seatnum,s)"
   ]
